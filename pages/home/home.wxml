<scroll-view class="container">
  <view class="base-info">
    <view class="line-set">
      <text>{{accountInfo.date}}</text>
      <button bindtap="goToAccount">记账</button>
    </view>
    <view class="line-set">
      <text>当月支出:{{accountInfo.monthExpenses}}元</text>
      <text>年度支出:{{accountInfo.annualExpenses}}元</text>
    </view>
    <view class="line-set">
      <text>预算金额:{{accountInfo.budget}}元</text>
      <text>消费金额:{{accountInfo.consumption}}元</text>
      <text>结余金额:{{accountInfo.balance}}元</text>
    </view>
  </view>
  <view class="account-img">
  </view>
  <view class="out-top">
  </view>
  <view class="month-out">
  </view>

</scroll-view>